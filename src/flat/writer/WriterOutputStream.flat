package flat/writer

import flat/compiler/models
import flat/io
import flat/log

class {
  static Logger log = Logger(WriterOutputStream.class)

  public write(String value = "") {
    log.info("writing string: #{value}")
  }

  public write(Token token) {
    log.info("writing token: #{token}")
  }

  public write(Token[] tokens) {
    log.info("writing tokens: #{tokens}")
  }
}